{
  "name": "n8n-nodes-aqbanking",
  "version": "0.1.0",
  "description": "n8n node to interact with German banks using AqBanking",
  "license": "MIT",
  "author": {
    "name": "CePEHH",
    "email": "cepehh@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CePeHH/n8n-nodes-aqbanking.git"
  },
  "main": "dist/nodes/AqBanking/AqBanking.node.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "format": "prettier --write \"**/*.ts\"",
    "lint": "eslint . --ext .ts --fix",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/AqBankingCredentials.credentials.js"
    ],
    "nodes": [
      "dist/nodes/AqBanking/AqBanking.node.js"
    ]
  },
  "dependencies": {
    "n8n-workflow": "*"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.50.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.3",
    "prettier": "^2.8.0",
    "typescript": "~4.8.4"
  }
}
